.baner-preorder
  position absolute
  top 0
  left 0
  right 0
  width 100%
  height 600px
  background-size cover
  background-position center
  background-repeat no-repeat

.section-title2
  font-family 'Merriweather Bold',sans-serif
  font-size 32px
  text-align center

.mt-35px
  margin-top 35px
