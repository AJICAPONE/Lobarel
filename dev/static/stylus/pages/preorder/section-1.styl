.selectCF {
  width: 100%;
}
.onCF li ul {
  display: block;
  margin-top: -3px;
  z-index: 11;
  background white
}
.select {
  width: 100%;
  position: relative;
}
.select-input {
  position: relative;
  background: rgba(40, 48, 55, 0.02);
  border: 2px solid rgba(40, 48, 55, 0.03);
  border-radius: 5px;
  height: 47px;
  width: 100%;
  padding: 14px;
  color: rgba(40, 48, 55, 0.61)
  font-family: "Open Sans 400", sans-serif;
  font-size: 14px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  cursor: pointer;
  z-index: 17;
}
.partner-wrap-input .select-input {
  height: 47px;
  font-size: 14px;
  z-index: 8;
}
.select-options {
  position: absolute;
  left: 0;
  right: 0;
  z-index: 21;
  display: none;
  border-top: 0;
}
.select-options li {
  font-size 14px
  padding: 15px 22px;
  background-color: #fff;
  font-family: "Open Sans 400", sans-serif;
  cursor: pointer;
  color: #283037;
  text-align: left;
}
.select-options li:last-child{
  border-radius: 0px 0px 5px 5px;
}
.append-icon .icon {
  position: absolute;
  right: 14px;
  width: 16px;
  height: 12px;
  fill: #283037;
}
.append-icon {
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  right: 0;
  top: 0;
  bottom: 0;
  height: 100%;
  width: 0px;
  z-index: 20;
}
.arrow-rotate {
  transform: rotate(180deg);
}

