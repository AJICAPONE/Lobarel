.harmonika-input-dot
  background: #FBFBFB;
  border 1px solid rgba(40, 48, 55, 0.1);
  size(19px)
  border-radius 50%
  margin-right 15px
  position relative
  display flex
  align-items center
  justify-content center
  opacity 1

.harmonika-input.checked  .harmonika-input-dot
  background transparent

.harmonika-input.checked .harmonika-input-name
  color #fff

.harmonika-input.checked
