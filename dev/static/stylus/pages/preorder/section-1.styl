.preorder-radio-checkbox-wrap
  display block
.preorder-radio-inputs--wrapper
  display flex
.preorder-radio-inputs
  display flex
  flex-direction column

  &:first-child
    margin-left -15px

.harmonika-preorder-input
  display none

.harmonika-input
  display flex
  align-items center
  background: rgba(40, 48, 55, 0.02);
  border-radius: 5px;
  padding 15px
  margin-bottom 15px
  cursor pointer


.harmonika-input-name
  font-family 'Merriweather Reg',sans-serif
  font-size 14px
  color rgba(40, 48, 55, 0.6)

.preorder-radio-input-div label
  width 100%
  margin-bottom 0

.preorder-radio-year
  font-family 'Open Sans 600',sans-serif
  font-size 12px
