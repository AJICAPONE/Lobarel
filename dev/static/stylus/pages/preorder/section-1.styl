.preorder-step-list
  display flex
  align-items center
  justify-content center
  margin-bottom 75px

.preorder-step
  display flex
  align-items center
  justify-content center
  position relative
  background-color #F9F9F9
  size(28px)
  border-radius 50%
  margin-right 160px
  font-family 'Merriweather Italic',sans-serif
  font-size 18px
  color rgba(40, 48, 55, 0.31)
  transition all 1s ease
  box-shadow 0px 0px 0 0px rgba(250, 172, 168, 0.31)

  &:last-child
    margin-right 0

  &:before
    content ''
    position absolute
    height 3px
    display: block;
    background: #eee;
    width 160px
    left 28px
    background: linear-gradient(to left, #eee 50%, #FAACA8 50%);
    background-size: 200% 100%;
    background-position: right bottom;
    transition all 1s ease

.preorder-step:last-child:before
  display none


.preorder-step.active
  background-color #FAACA8
  box-shadow 0px 0px 0 3px rgba(250, 172, 168, 0.31)
  color #fff


.preorder-step.finish
  background #FAACA8
  color #fff

  &:before
    background-position: left bottom;

.form-preorder-wrapper
  box-shadow: 0px 0px 40px rgba(0, 0, 0, 0.1);
  border-radius: 10px;
  background-color #fff
  padding 50px 30px

.preorder-tab
  display: none;


.preorder-title
  display flex
  align-items center
  justify-content center
  margin-bottom 50px
.preorder-title-step
  font-family 'Merriweather Italic',sans-serif
